{
  "name": "tifffs-donut",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'",
    "knex": "knex --knexfile server/db/knexfile.js",
    "test": "vitest server",
    "playwright": "npx playwright test"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ],
    "rules": {
      "react/no-unknown-property": "off"
    }
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.1",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@react-three/drei": "9.75.0",
    "@react-three/fiber": "8.13.3",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@types/three": "0.152.1",
    "express": "^4.18.2",
    "express-jwt": "^7.7.5",
    "express-jwt-authz": "^2.4.1",
    "framer-motion-3d": "^10.16.4",
    "gsap": "^3.12.2",
    "jwks-rsa": "^2.1.5",
    "knex": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "react-scroll-to-top": "^3.0.0",
    "sqlite3": "^5.1.6",
    "superagent": "^8.1.2",
    "three": "0.153.0"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.9.1",
    "@playwright/test": "^1.38.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "run-s": "^0.0.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^4.1.0",
    "vitest": "^0.34.4"
  }
}
